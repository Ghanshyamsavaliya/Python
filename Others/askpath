from tkinter import *
root = Tk()

class abc:
    def path():
        global Ghanshyam
        string = Ghanshyam.get() 
        pathing = print ("r"+'"'+string+'"')
        Ghanshyam.destroy()
        root.destroy()
    
root.geometry("500x500")
root.iconbitmap('Global.ico')
root.title('Name')
Ghanshyam = Entry(root, bd=4)
Ghanshyam.pack()
Ghanshyam.focus_set()
b = Button(root,text='okay',command=abc.path)
b.pack(side='bottom')
root.mainloop()
